    "directory": "packages/gatsby-plugin-flow"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/preset-flow": "^7.24.7",
    "@babel/runtime": "^7.25.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "babel-preset-gatsby-package": "^2.22.0-next.0",
    "cross-env": "^7.0.3",
    "gatsby-plugin-utils": "^3.16.0-next.1"
  },
